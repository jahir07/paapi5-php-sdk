{
    "name" : "jahir07/paapi5-php-sdk",
    "description" : "Providing the official of Amazon Product Advertising API v5 SDK for PHP as composer package.",
    "type": "library",
    "version" : "1.0.1",
    "keywords" : [
        "amazon",
        "php",
        "sdk",
        "api"
    ],
    "homepage" : "https://webservices.amazon.com/paapi5/documentation/index.html",
    "license" : "Apache-2.0",
    "authors" : [ { "name" : "Amazon Product Advertising API Team" } ],
    "require" : {
        "php" : ">=5.5",
        "guzzlehttp/guzzle" : "^6.2|^7.0"
    },
    "autoload": {
        "psr-4": { "Amazon\\ProductAdvertisingAPI\\v1\\" : "src/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
